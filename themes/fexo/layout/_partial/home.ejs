<div class="content-home text-center">
  <a class="avatar" href="/">
    <img src="<%= theme.avatar %>" alt="" />
  </a>
  <h1 class="name" style="font-family: <%= theme.blog_name_font_familiy || 'calligraffittiregular' %>">
    <%= theme.blog_name %>
  </h1>
  <div class="slogan">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
    <script src="js/typed.js" type="text/javascript"></script>
    <script>
      $(function(){
      $("#typed").typed({
        strings: ["Computer Science","Connoisseur of Code","Machine Learning","Mathematics","Full Stack Developer","Starbucks~","swift...and Swift","Macintosh","Go Colonels!","Nap King"],
          typeSpeed: 25,
          backDelay: 1000,
          loop: true,
           });
         });
    </script>

  <span id="typed"></span>
  </div>

  <hr>

  <ul class="text-center nav">
    <% theme.home_nav.forEach(function(item, i){ %>
      <li class="item">
        <% if(item.target_blank === true) { %>
          <a href="<%= item.url %>" rel="noopener noreferrer" target="_blank"><%= item.name %></a>
        <% } else { %>
          <a href="<%= item.url %>"><%= item.name %></a>
        <% } %>
        <span>|</span>
      </li>
    <% }) %>
  </ul>

</div>
